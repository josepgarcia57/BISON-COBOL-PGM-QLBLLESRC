           EXEC SQL DECLARE CURJDFIN CURSOR FOR
            SELECT    DTREF
                   ,  BPENV
                   ,  INFDINSTF
                   ,  IDCONT
                   ,  IDINST
                   ,  TAADIA
                   ,  CAPITALV
                   ,  TMSCRIA
                   ,  USERCRIA
                   ,  TMSALT
                   ,  USERALT
                FROM  BPCJDFIN A
                WHERE BPENV = "E"
           END-EXEC.

           EXEC SQL DECLARE CURJDFIN1 CURSOR FOR
            SELECT   IDCONT
                  ,  IDINST
                  ,  IDENT
                  ,  TYPER
                  ,  MTOTDIA
                  ,  MVENCDIA
                  ,  MABATDIA
                  ,  MPOTRDIA
                  ,  MPOTIRDIA
                  ,  TPEVENTDIA
                  ,  TPRESPDIA
                  ,  TMSCRIA
                  ,  USERCRIA
                  ,  TMSALT
                  ,  USERALT
                FROM  BPCJDFIN1
                WHERE IDCONT = :WS-IDCONT
                 AND  IDINST = :WS-IDINST
           END-EXEC.

           EXEC SQL END  DECLARE SECTION END-EXEC.
