     š* Cursor Principal
           EXEC SQL DECLARE CURJCCIN CURSOR FOR
            SELECT    A.INFTINST
                   ,  A.BPENV
                   ,  A.DTREF
                   ,  A.IDCONT
                   ,  A.IDINST
                   ,  A.BALCAO
                   ,  A.PROJFINAN
                   ,  A.IDCONTSIND
                   ,  A.LITIGJUD
                   ,  A.IEB
                   ,  A.PAISLEGIS
                   ,  A.CANALCOMER
                   ,  A.CLAUSRENUN
                   ,  A.SUBVPROT
                   ,  A.REFEXTINST
                   ,  A.TPINST
                   ,  A.MOEDA
                   ,  A.DTUTILFUND
                   ,  A.DTINIINST
                   ,  A.DTORIMAT
                   ,  A.DTMAT
                   ,  A.DTINICRJUR
                   ,  A.DTFIMCRJUR
                   ,  A.DTINICRCAP
                   ,  A.DTFIMCRCAP
                   ,  A.DIRREMBIM
                   ,  A.RECURSO
                   ,  A.TPTXJUR
                   ,  A.FREQACTTX
                   ,  A.TXREF
                   ,  A.SPREAD
                   ,  A.TXMAX
                   ,  A.TXMIN
                   ,  A.TAEG
                   ,  A.TAE
                   ,  A.PERFIXTX
                   ,  A.DURPLNFIN
                   ,  A.FINALIDADE
                   ,  A.TPAMORT
                   ,  A.FREQPAGAM
                   ,  A.DIVSUBOR
                   ,  A.INSTFIDUC
                   ,  A.MONTINI
                   ,  A.VARFV
                   ,  VALUE(B.DTRENEG , " " )
                   ,  VALUE(B.TPRENEG , "001" )
                   ,  A.PERCDIFCAP
                   ,  A.TPTITUL
                   ,  A.SEGUROS
                   ,  A.TMSCRIA
                   ,  A.USERCRIA
                   ,  A.TMSALT
                   ,  A.USERALT
                FROM  BPCJCCIN A
                 LEFT JOIN BPCTRENEG B
                  ON ( A.IDCONT = B.IDCONT
                   AND A.IDINST = B.IDINST  )
                WHERE BPENV = "E"
           END-EXEC.

