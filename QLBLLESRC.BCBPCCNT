     š*--Selecção de acordo com a Tabela 30 de Produtos
     š*--Retirado o Acesso aos Contratos - Nos DP'S
           EXEC SQL DECLARE CURFDBOPE CURSOR FOR
                SELECT
                        CPRACI
                     ,  CPGRE
                     ,  CPRUB
                     ,  CPMON
                     , "000"
                     , "0000000"
                FROM  FDBCPT A
                WHERE CPGRE  IN( "001"
                               , "007"
                               , "404"
                               , "405"
                               , "446"
                               , "400"
                               , "450"  )
                AND ( CPDTAN  = " " OR
                      CPDTAN  = "000000"
                      OR  ("20" ||
                          SUBSTR(CPDTAN , 5 , 2 ) || "-" ||
                          SUBSTR(CPDTAN , 3 , 2 ) || "-" ||
                          SUBSTR(CPDTAN , 1 , 2 ) > :WS-DATA-INI-MES))
                     AND CPRUB = "000"
                     AND (CPMON = "978" OR
                          CPGRE  IN( "400" , "450" ))
                    UNION ALL
                SELECT
                        CTRACI
                     ,  CTGRE
                     ,  CTRUB
                     ,  CTMON
                     ,  CTID
                     ,  CTCONT
                FROM  FDBCTR A
                WHERE CTGRE  IN( "402"
                               , "403"
                               , "438"  )
                AND CTOPER NOT IN "L"
                AND (CTDTLQ  = " "  OR CTDTLQ = "000000"
                     OR  ("20" ||
                          SUBSTR(CTDTLQ , 5 , 2 ) || "-" ||
                          SUBSTR(CTDTLQ , 3 , 2 ) || "-" ||
                          SUBSTR(CTDTLQ , 1 , 2 ) > :WS-DATA-INI-MES))
                     AND CTRUB = "000"
                    UNION ALL
                SELECT
                        DOCLIR
                     , "000"
                     ,  DOCLIS
                     , "000"
                     , "000"
                     , "0000000"
                FROM  FDBDOS A
                WHERE (DODTAN  = " "  OR DODTAN = "000000"
                      OR  ("20" ||
                          SUBSTR(DODTAN , 5 , 2 ) || "-" ||
                          SUBSTR(DODTAN , 3 , 2 ) || "-" ||
                          SUBSTR(DODTAN , 1 , 2 ) > :WS-DATA-INI-MES))
                  AND DOCLIS = "000"
           END-EXEC.

           EXEC SQL END  DECLARE SECTION END-EXEC.
