     š*---------------------------------------------------------------------
       P003-VERIFICA-NR-ENVIO.
     š*---------------------------------------------------------------------
     š*- Último Envio

           EXEC SQL
             SELECT     X.ANO
                     ,  X.MES
                     ,  X.VERSAOBCB
                     ,  X.REMET
                     ,  X.REPOR
                     ,  X.DESTI
                     ,  X.DTCRIACAO
                     ,  X.VERSAO
                     ,  X.VERSUB
                     ,  X.NOMEFICH
                     ,  X.ESTADO
                     ,  X.NREC
                     ,  X.NREJ
                     ,  X.NINT
                     ,  X.NALERTA
                     ,  X.TMSCRIA
                     ,  X.TMSRET
             INTO      :BCBJCNTL.ANO
                     , :BCBJCNTL.MES
                     , :BCBJCNTL.VERSAOBCB
                     , :BCBJCNTL.REMET
                     , :BCBJCNTL.REPOR
                     , :BCBJCNTL.DESTI
                     , :BCBJCNTL.DTCRIACAO
                     , :BCBJCNTL.VERSAO
                     , :BCBJCNTL.VERSUB
                     , :BCBJCNTL.NOMEFICH
                     , :BCBJCNTL.ESTADO
                     , :BCBJCNTL.NREC
                     , :BCBJCNTL.NREJ
                     , :BCBJCNTL.NINT
                     , :BCBJCNTL.NALERTA
                     , :BCBJCNTL.TMSCRIA
                     , :BCBJCNTL.TMSRET
             FROM  BCBJCNTL  X
                WHERE ANO    = :BCBJCNTL.ANO
                 AND  MES    = :BCBJCNTL.MES
                 AND  TMSCRIA = ( SELECT MAX(TMSCRIA)
                                  FROM BCBJCNTL Y
                                  WHERE X.MES = Y.MES
                                   AND  X.ANO = Y.ANO )
           END-EXEC.

           MOVE SQLCODE  TO  W-SQLCODE, W-EDTSQLC.
           EVALUATE TRUE
           WHEN SQLCODE-OK
              MOVE VERSAO OF BCBJCNTL TO WX-NOVA-VERSAO
              IF VERSUB OF BCBJCNTL NOT EQUAL "1" AND
                 ESTADO OF BCBJCNTL  = "RET"
                 ADD 1        TO WS-NOVA-VERSAO
                 MOVE "1"     TO VERSUB OF BCBJCNTL
              ELSE
                PERFORM P003-DELETE-BCBJCNTL
              END-IF
           WHEN SQLCODE-NOTFOUND
              MOVE  1        TO WS-NOVA-VERSAO
              MOVE "0"       TO VERSUB OF BCBJCNTL
           WHEN OTHER
              MOVE "BCBJCNTL"        TO W-OBJECT-NAME
              SET CMD-SELECT         TO TRUE
              PERFORM  P990-ERRO-DB2
           END-EVALUATE.

     š*---------------------------------------------------------------------
       P003-DELETE-BCBJCNTL.
     š*---------------------------------------------------------------------
           EXEC SQL
             DELETE FROM BCBJCNTL
             WHERE  ANO    = :BCBJCNTL.ANO
               AND  MES    = :BCBJCNTL.MES
               AND  VERSAO = :BCBJCNTL.VERSAO
           END-EXEC.

           MOVE SQLCODE  TO  W-SQLCODE, W-EDTSQLC.

           EVALUATE TRUE
           WHEN SQLCODE-OK
             CONTINUE
           WHEN OTHER
              MOVE "BCBJCNTL"          TO W-OBJECT-NAME
              SET CMD-INSERT           TO TRUE
              PERFORM  P990-ERRO-DB2
           END-EVALUATE.

     š*---------------------------------------------------------------------
       P003-INSERT-BCBJCNTL.
     š*---------------------------------------------------------------------
     š*-
           EXEC SQL
                 INSERT INTO BCBJCNTL
                 VALUES ( :BCBJCNTL.ANO
                        , :BCBJCNTL.MES
                        , :BCBJCNTL.VERSAOBCB
                        , :BCBJCNTL.REMET
                        , :BCBJCNTL.REPOR
                        , :BCBJCNTL.DESTI
                        , :BCBJCNTL.DTCRIACAO
                        , :BCBJCNTL.VERSAO
                        , :BCBJCNTL.VERSUB
                        , :BCBJCNTL.NOMEFICH
                        , :BCBJCNTL.ESTADO
                        , :BCBJCNTL.NREC
                        , :BCBJCNTL.NREJ
                        , :BCBJCNTL.NINT
                        , :BCBJCNTL.NALERTA
                        , CURRENT TIMESTAMP
                        , :BCBJCNTL.TMSRET )
           END-EXEC.

           MOVE SQLCODE  TO  W-SQLCODE, W-EDTSQLC.

           EVALUATE TRUE
           WHEN SQLCODE-OK
             CONTINUE
           WHEN OTHER
              MOVE "BCBJCCNT"          TO W-OBJECT-NAME
              SET CMD-INSERT           TO TRUE
              PERFORM  P990-ERRO-DB2
           END-EVALUATE.

