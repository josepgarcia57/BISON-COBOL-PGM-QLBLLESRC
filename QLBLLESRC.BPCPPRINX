     š* Cursor Principal / Contratos Instrumentos com Exposições
           EXEC SQL DECLARE CURJPR CURSOR FOR
            SELECT    DTREF
                   ,  infRInst
                   ,  IDCONT
                   ,  IDINST
                FROM  BPCJPRIN A
                WHERE BPENV = "E"
                 AND  IDEXP <> " "
                GROUP BY DTREF
                       , infRInst
                       , IDCONT
                       , IDINST
           END-EXEC.

     š* Cursor Principal / Exposções
           EXEC SQL DECLARE CURJPRIN CURSOR FOR
            SELECT    DTREF
                   ,  BPENV
                   ,  infRInst
                   ,  IDCONT
                   ,  IDINST
                   ,  IDEXP
                   ,  TPEXP
                   ,  CLASEXPCRR
                   ,  METCALCFPR
                   ,  VAJUSTCOLF
                   ,  MPONDEXPR
                   ,  RISCOPOND
                   ,  LGDPERECN
                   ,  LGDREC
                   ,  VALEXP
                   ,  PREFCONV
                   ,  MONTPERESP
                   ,  EXPAJPME
                   ,  FCONVCRED
                   ,  TMSCRIA
                   ,  USERCRIA
                   ,  TMSALT
                   ,  USERALT
                FROM  BPCJPRIN A
                WHERE BPENV = "E"
                  AND IDCONT = :WS-IDCONT
                  AND IDINST = :WS-IDINST
           END-EXEC.

     š* Cursor Principal / Protecções das Exposições
           EXEC SQL DECLARE CURJPRIN1 CURSOR FOR
            SELECT    IDCONT
                   ,  IDINST
                   ,  IDEXP
                   ,  IDPROT
                FROM  BPCJPRIN1 A
               WHERE IDCONT = :WS-IDCONT
                 AND IDINST = :WS-IDINST
                 AND IDEXP  = :PRIN-IDEXP
           END-EXEC.

           EXEC SQL END  DECLARE SECTION END-EXEC.
