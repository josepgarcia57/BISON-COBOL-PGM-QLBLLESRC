      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-DESC-STATEXPROT       SECTION.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
           INITIALIZE BPCB002-LKPARMS

           MOVE CREB950-TBP-EEXECG    TO BPCB002-CTAB
           MOVE BPCB352-STATEXPROT-V  TO BPCB002-CACC

           CALL "BPCS002" USING BPCB002-LKPARMS

           IF BPCB002-PROC-OK
              MOVE BPCB002-CAMPO1          TO BPCB352-NMSTATEX-V
              IF BPCB002-CAMPO1 = SPACES
                 MOVE BPCB002-DESC         TO BPCB352-NMSTATEX-V
              END-IF
           ELSE
              SET  CREB950-VCAMPO-INVALID  TO TRUE
              SET  BPCB352-STATEXPROT-PC   TO TRUE
              SET  CREB000-MSGFUNC         TO TRUE
           END-IF
           .
       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-DESC-IDENTPROT        SECTION.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
           MOVE  BPCB352-IDPROT-V(1:7) TO WS-NURACI

           EXEC SQL
                SELECT  CLRACI
                     ,  CLNOM
                     ,  CLPRNM
                INTO   :CLRACI
                     , :CLNOM
                     , :CLPRNM
                FROM  FDBCLI A
                WHERE A.CLRACI = :WS-NURACI
           END-EXEC.

           MOVE SQLCODE TO CREB950-SQLCODE

           EVALUATE TRUE
           WHEN SQLCODE-OK
              PERFORM P9000-NOME
              MOVE WS-NOM-COMPLETO      TO BPCB352-NMIDENT-V
           WHEN SQLCODE-NOTFOUND
              SET  CREB950-VCAMPO-INVALID  TO TRUE
              SET  BPCB352-IDENTPROT-PC    TO TRUE
              SET  CREB000-MSGFUNC         TO TRUE
           WHEN OTHER
               MOVE CREB950-SQLCODE     TO CREB000-CDRETORN
               MOVE "CREP909.FDBCLI"    TO CREB000-NMOBJECT
               SET  CREB000-SLCT        TO TRUE
               SET  CREB000-MSGSGBD     TO TRUE
           END-EVALUATE.

       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-NOME  SECTION.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
           MOVE  CLNOM     OF R-FDBCLI   TO WS-NOM
           MOVE  CLPRNM    OF R-FDBCLI   TO WS-PRNM
           MOVE ZEROS TO WS-POS-NOMI WS-POS-PRNMI

           INSPECT FUNCTION REVERSE(WS-NOM)
                   TALLYING WS-POS-NOMI     FOR LEADING SPACE.
           COMPUTE  WS-NOM-L    = LENGTH OF WS-NOM  -  WS-POS-NOMI.

           INSPECT FUNCTION REVERSE(WS-PRNM)
                   TALLYING WS-POS-PRNMI    FOR LEADING SPACE.
           COMPUTE  WS-PRNM-L   = LENGTH OF WS-PRNM - WS-POS-PRNMI.

           MOVE SPACES                    TO WS-NOM-COMPLETO
           IF WS-PRNM-L > 0
             MOVE WS-PRNM(1 :WS-PRNM-L)   TO WS-NOM-COMPLETO
           END-IF
           IF WS-NOM-L  > 0
             ADD 1 TO WS-PRNM-L
             MOVE WS-NOM(1:WS-NOM-L)
                             TO WS-NOM-COMPLETO(WS-PRNM-L:WS-NOM-L)
           END-IF.
       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-FRMT-BPCTCPRT-CHAV     SECTION.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *

           MOVE BPCB352-IDPROT-V       TO IDPROT   OF BPCTCPRT
           IF  CREB000-DOMODFY OR CREB000-DONULFY
               MOVE CREB000-DHMDFREG   TO TMSALT   OF BPCTCPRT
           END-IF
           .
       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-FRMT-BPCTCPRT-ATRB     SECTION.

           MOVE BPCB352-IDPROT-V      TO IDPROT     OF BPCTCPRT
           MOVE BPCB352-IDENTPROT-V   TO IDENT      OF BPCTCPRT
           MOVE BPCB352-TYPER-V       TO TYPER      OF BPCTCPRT
           MOVE BPCB352-TPPROT-V      TO TPPROT     OF BPCTCPRT
           MOVE BPCB352-REFEXTPROT-V  TO REFEXTPROT OF BPCTCPRT
           MOVE BPCB352-IDPROT-V      TO IDPROT     OF BPCTCPRT
           MOVE BPCB352-VALPROT-V     TO VALPROT    OF BPCTCPRT
           MOVE BPCB352-TPVALPROT-V   TO TPVALPROT  OF BPCTCPRT
           MOVE BPCB352-DTMATPROT-V   TO DTMATPROT  OF BPCTCPRT
           MOVE BPCB352-PAISLPROT-V   TO PAISLPROT  OF BPCTCPRT
           MOVE BPCB352-TPVALPROT-V   TO TPVALPROT  OF BPCTCPRT
           MOVE BPCB352-DTMATPROT-V   TO DTMATPROT  OF BPCTCPRT
           MOVE BPCB352-PAISLPROT-V   TO PAISLPROT  OF BPCTCPRT
           MOVE BPCB352-REGLOCPROT-V  TO REGLOCPROT OF BPCTCPRT
           MOVE BPCB352-DTULTAVAL-V   TO DTULTAVAL  OF BPCTCPRT
           MOVE BPCB352-TPVALPROT-V   TO TPAVAL     OF BPCTCPRT
           MOVE BPCB352-VALORIPROT-V  TO VALORIPROT OF BPCTCPRT
           MOVE BPCB352-DTVALPRTO-V   TO DTVALPRTO  OF BPCTCPRT
           MOVE BPCB352-HIERQPROT-V   TO HIERQPROT  OF BPCTCPRT
           MOVE BPCB352-PRECOAQUIS-V  TO PRECOAQUIS OF BPCTCPRT
           MOVE BPCB352-HIERQPROT-V   TO NUMREGPROT OF BPCTCPRT
           MOVE BPCB352-STATEXPROT-V  TO STATEXPROT OF BPCTCPRT
           MOVE BPCB352-DTEXECPROT-V  TO DTEXECPROT OF BPCTCPRT
           MOVE BPCB352-VALACEXEC-V   TO VALACEXEC  OF BPCTCPRT

           EVALUATE TRUE
           WHEN CREB000-DOCREAT
               MOVE CREB000-IDUTILIZ   TO USERCRIA OF BPCTCPRT
               MOVE CREB950-TMSTPMIN   TO TMSCRIA  OF BPCTCPRT
           WHEN CREB000-DOMODFY
               MOVE CREB000-IDUTILIZ   TO USERALT  OF BPCTCPRT
           END-EVALUATE
           CONTINUE
           .
       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-INSR-BPCTCPRT          SECTION.

           EXEC SQL INSERT INTO BPCTCPRT
                   (       IDPROT
                         , IDENT
                         , TYPER
                         , TPPROT
                         , REFEXTPROT
                         , IDINST
                         , VALPROT
                         , TPVALPROT
                         , DTMATPROT
                         , PAISLPROT
                         , REGLOCPROT
                         , DTULTAVAL
                         , TPAVAL
                         , VALORIPROT
                         , DTVALPRTO
                         , HIERQPROT
                         , PRECOAQUIS
                         , NUMREGPROT
                         , STATEXPROT
                         , TMSCRIA
                         , USERCRIA
                         , TMSALT
                         , USERALT
                         )
                    VALUES
                         ( :BPCTCPRT.IDPROT
                         , :BPCTCPRT.IDENT
                         , :BPCTCPRT.TYPER
                         , :BPCTCPRT.TPPROT
                         , :BPCTCPRT.REFEXTPROT
                         , :BPCTCPRT.IDINST
                         , :BPCTCPRT.VALPROT
                         , :BPCTCPRT.TPVALPROT
                         , :BPCTCPRT.DTMATPROT
                         , :BPCTCPRT.PAISLPROT
                         , :BPCTCPRT.REGLOCPROT
                         , :BPCTCPRT.DTULTAVAL
                         , :BPCTCPRT.TPAVAL
                         , :BPCTCPRT.VALORIPROT
                         , :BPCTCPRT.DTVALPRTO
                         , :BPCTCPRT.HIERQPROT
                         , :BPCTCPRT.PRECOAQUIS
                         , :BPCTCPRT.NUMREGPROT
                         , :BPCTCPRT.STATEXPROT
                         , :BPCTCPRT.TMSCRIA
                         , :BPCTCPRT.USERCRIA
                         , :BPCTCPRT.TMSALT
                         , :BPCTCPRT.USERALT
                         )                                     END-EXEC
           MOVE SQLCODE TO CREB950-SQLCODE
           IF  NOT SQLCODE-OK
               MOVE CREB950-SQLCODE   TO CREB000-CDRETORN
               MOVE "BPCP302.BPCTCPRT" TO CREB000-NMOBJECT
               SET  CREB000-INSR      TO TRUE
               SET  CREB000-MSGSGBD   TO TRUE
           END-IF
           .
       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-UPDT-BPCTCPRT          SECTION.

           EXEC SQL UPDATE BPCTCPRT
                       SET IDPROT      =:BPCTCPRT.IDPROT
                         , IDENT       =:BPCTCPRT.IDENT
                         , TYPER       =:BPCTCPRT.TYPER
                         , TPPROT      =:BPCTCPRT.TPPROT
                         , REFEXTPROT  =:BPCTCPRT.REFEXTPROT
                         , VALPROT     =:BPCTCPRT.VALPROT
                         , TPVALPROT   =:BPCTCPRT.TPVALPROT
                         , DTMATPROT   =:BPCTCPRT.DTMATPROT
                         , PAISLPROT   =:BPCTCPRT.PAISLPROT
                         , REGLOCPROT  =:BPCTCPRT.REGLOCPROT
                         , DTULTAVAL   =:BPCTCPRT.DTULTAVAL
                         , TPAVAL      =:BPCTCPRT.TPAVAL
                         , VALORIPROT  =:BPCTCPRT.VALORIPROT
                         , DTVALPRTO   =:BPCTCPRT.DTVALPRTO
                         , HIERQPROT   =:BPCTCPRT.HIERQPROT
                         , PRECOAQUIS  =:BPCTCPRT.PRECOAQUIS
                         , NUMREGPROT  =:BPCTCPRT.NUMREGPROT
                         , STATEXPROT  =:BPCTCPRT.STATEXPROT
                         , TMSALT      = CURRENT TIMESTAMP
                         , USERALT     =:BPCTCPRT.USERALT
                     WHERE IDPROT   = :BPCTCPRT.IDPROT
                       AND TMSALT   = :BPCTCPRT.TMSALT       END-EXEC

           MOVE SQLCODE TO CREB950-SQLCODE
           EVALUATE TRUE
           WHEN SQLCODE-OK
               CONTINUE
           WHEN SQLCODE-NOTFOUND
               SET  CREB950-FUNCAO-INTERRP TO TRUE
               SET  CREB000-MSGFUNC        TO TRUE
           WHEN OTHER
               MOVE CREB950-SQLCODE        TO CREB000-CDRETORN
               MOVE "BPCP302.BPCTCPRT"      TO CREB000-NMOBJECT
               SET  CREB000-UPDT           TO TRUE
               SET  CREB000-MSGSGBD        TO TRUE
           END-EVALUATE
           .
       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-DELT-BPCTCPRT          SECTION.

           EXEC SQL DELETE FROM BPCTCPRT
                     WHERE IDINST   = :BPCTCPRT.IDINST
                       AND TMSALT   = :BPCTCPRT.TMSALT        END-EXEC.

           MOVE SQLCODE TO CREB950-SQLCODE
           EVALUATE TRUE
           WHEN SQLCODE-OK
               CONTINUE
           WHEN SQLCODE-NOTFOUND
               SET  CREB950-FUNCAO-INTERRP TO TRUE
               SET  CREB000-MSGFUNC        TO TRUE
           WHEN OTHER
               MOVE CREB950-SQLCODE        TO CREB000-CDRETORN
               MOVE "BPCP302.BPCTCPRT"      TO CREB000-NMOBJECT
               SET  CREB000-DELT           TO TRUE
               SET  CREB000-MSGSGBD        TO TRUE
           END-EVALUATE
           .
       P9000-EXIT.                 EXIT.
      *--- FIM DO PROGRAMA BPCP302 ------------------------------------*
