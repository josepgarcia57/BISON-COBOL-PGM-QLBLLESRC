      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-READ-BPCTCPRT          SECTION.

           MOVE BPCB352-IDPROT-V      TO IDPROT   OF BPCTCPRT
           EXEC SQL SELECT
                           IDPROT
                         , IDENT
                         , TYPER
                         , TPPROT
                         , REFEXTPROT
                         , IDINST
                         , VALPROT
                         , TPVALPROT
                         , DTMATPROT
                         , PAISLPROT
                         , TPVALPROT
                         , DTMATPROT
                         , PAISLPROT
                         , REGLOCPROT
                         , DTULTAVAL
                         , TPAVAL
                         , VALORIPROT
                         , DTVALPRTO
                         , HIERQPROT
                         , PRECOAQUIS
                         , NUMREGPROT
                         , STATEXPROT
                         , DTEXECPROT
                         , TMSCRIA
                         , USERCRIA
                         , TMSALT
                         , USERALT
                      INTO :BPCTCPRT.IDPROT
                         , :BPCTCPRT.IDENT
                         , :BPCTCPRT.TYPER
                         , :BPCTCPRT.TPPROT
                         , :BPCTCPRT.REFEXTPROT
                         , :BPCTCPRT.IDINST
                         , :BPCTCPRT.VALPROT
                         , :BPCTCPRT.TPVALPROT
                         , :BPCTCPRT.DTMATPROT
                         , :BPCTCPRT.PAISLPROT
                         , :BPCTCPRT.TPVALPROT
                         , :BPCTCPRT.DTMATPROT
                         , :BPCTCPRT.PAISLPROT
                         , :BPCTCPRT.REGLOCPROT
                         , :BPCTCPRT.DTULTAVAL
                         , :BPCTCPRT.TPAVAL
                         , :BPCTCPRT.VALORIPROT
                         , :BPCTCPRT.DTVALPRTO
                         , :BPCTCPRT.HIERQPROT
                         , :BPCTCPRT.PRECOAQUIS
                         , :BPCTCPRT.NUMREGPROT
                         , :BPCTCPRT.STATEXPROT
                         , :BPCTCPRT.DTEXECPROT
                         , :BPCTCPRT.TMSCRIA
                         , :BPCTCPRT.USERCRIA
                         , :BPCTCPRT.TMSALT
                         , :BPCTCPRT.USERALT
                      FROM BPCTCPRT
                     WHERE IDPROT   = :BPCTCPRT.IDPROT       END-EXEC.

           MOVE SQLCODE TO CREB950-SQLCODE
           EVALUATE TRUE
           WHEN SQLCODE-OK
               SET  CREB000-IDREG-OK  TO TRUE
           WHEN SQLCODE-NOTFOUND
               SET  CREB000-IDREG-NF  TO TRUE
           WHEN OTHER
               MOVE CREB950-SQLCODE     TO CREB000-CDRETORN
               MOVE "BPCP302.BPCTCPRT" TO CREB000-NMOBJECT
               SET  CREB000-SLCT        TO TRUE
               SET  CREB000-MSGSGBD     TO TRUE
           END-EVALUATE
           .
       P9000-EXIT.                 EXIT.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-DESC-RUB              SECTION.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
           MOVE  BPCB352-IDPROT-V(9:3) TO RUB-PROT

           EVALUATE RUB-PROT
           WHEN "560"
             MOVE "560 - Garantia Recebida"
                   TO BPCB352-NMPROT-V
           WHEN "561"
             MOVE "561 - Garantia Recebida Outros Bancos"
                   TO BPCB352-NMPROT-V
           WHEN "562"
             MOVE "562 - Garantia Recebida Imóveis"
                   TO BPCB352-NMPROT-V
           WHEN "563"
             MOVE "563 - Garantia Recebida Penhor Equipamento"
                   TO BPCB352-NMPROT-V
           WHEN "564"
             MOVE "564 - Garantia Recebida Carteira no BBI"
                   TO BPCB352-NMPROT-V
           WHEN "565"
             MOVE "565 - Garantia Recebida Avales"
                   TO BPCB352-NMPROT-V
           WHEN "566"
             MOVE "565 - Garantia Recebida do Estado"
                   TO BPCB352-NMPROT-V
