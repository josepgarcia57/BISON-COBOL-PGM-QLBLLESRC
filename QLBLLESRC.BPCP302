      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
       P9000-DESC-TYPER            SECTION.
      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
           EVALUATE BPCB352-TYPER-V
           WHEN "i1"
             MOVE "NIF/NIPC"             TO BPCB352-NMTYPER-V
           WHEN "i2"
