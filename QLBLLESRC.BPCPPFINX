     š* Cursor Principal
           EXEC SQL DECLARE CURJPFIN CURSOR FOR
            SELECT    DTREF
                   ,  BPENV
                   ,  INFFININST
                   ,  IDCONT
                   ,  IDINST
                   ,  TAA
                   ,  TAN
                   ,  COMDESPBAL
                   ,  COMDESPEXP
                   ,  CREDALARG
                   ,  CREDCONV
                   ,  DTACTTXJUR
                   ,  DTESTINC
                   ,  DTINSTVENC
                   ,  ESTINC
                   ,  INSTFINAL
                   ,  JURCORR
                   ,  JURVENCBAL
                   ,  JURVENCEXT
                   ,  MONTABATV
                   ,  MPOTIRREV
                   ,  MPOTREV
                   ,  MREEMBANT
                   ,  MONTTRANSF
                   ,  MONTVENC
                   ,  MONTVIVO
                   ,  TPREEMBANT
                   ,  VALPREST
                   ,  TMSCRIA
                   ,  USERCRIA
                   ,  TMSALT
                   ,  USERALT
                FROM  BPCJPFIN A
                WHERE BPENV = "E"
                 AND  JURCORR
                    + JURVENCBAL
                    + JURVENCEXT
                    + MONTABATV
                    + MPOTIRREV
                    + MPOTREV
                    + MREEMBANT
                    + MONTTRANSF
                    + MONTVENC
                    + MONTVIVO
                    + VALPREST    <> 0
           END-EXEC.

     š* Cursor Entidades
           EXEC SQL DECLARE CURJPFIN1 CURSOR FOR
            SELECT   IDCONT
                  ,  IDINST
                  ,  IDENT
                  ,  TYPER
                  ,  TPRESPENT
                  ,  MTOTENT
                  ,  MVENCENT
                  ,  MPOTRVENT
                  ,  MPOTIRVENT
                  ,  MABATENT
                  ,  VPRESTENT
                  ,  TMSCRIA
                  ,  USERCRIA
                  ,  TMSALT
                  ,  USERALT
                FROM  BPCJPFIN1
                WHERE IDCONT = :WS-IDCONT
                 AND  IDINST = :WS-IDINST
                 AND  IDENT <> " "
           END-EXEC.
     š* Cursor Protecções

           EXEC SQL DECLARE CURJPFIN2 CURSOR FOR
            SELECT   IDCONT
                  ,  IDINST
                  ,  IDPROT
                  ,  VALPROT
                  ,  CREDPRIOR
                  ,  INDEXECPRT
                  ,  VALEXECPRT
                  ,  TMSCRIA
                  ,  USERCRIA
                  ,  TMSALT
                  ,  USERALT
                FROM  BPCJPFIN2
                WHERE IDCONT = :WS-IDCONT
                 AND  IDINST = :WS-IDINST
           END-EXEC.

