      *- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - *
           EXEC SQL DECLARE QPROCES CURSOR FOR
                    SELECT IDPRCAPL
                         , DSPRCAPL
                         , IDAPLICA
                         , IDPRGGST
                         , IDDIALOG
                         , IDTIPDLG
                         , MCSITEXE
                      FROM CRET903
                     WHERE IDPRCAPL LIKE :DB-PARMSINP.DB-IDPRCAPL
                       AND DSPRCAPL LIKE :DB-PARMSINP.DB-DSPRCAPL
                       AND(IDAPLICA    = :DB-PARMSINP.DB-IDAPLICA
                        OR "N"         = :DB-IDAPLFLT)
                       AND(IDDIALOG    = :DB-PARMSINP.DB-IDDIALOG
                        OR "N"         = :DB-IDDLGFLT)
                       AND(IDTIPDLG    = :DB-PARMSINP.DB-IDTIPDLG
                        OR "N"         = :DB-TPDLGFLT)
                       AND(MCSITEXE    = :DB-PARMSINP.DB-MCSITEXE
                        OR "N"         = :DB-STEXEFLT)
                       AND(IDPRCDEP    = :DB-PARMSINP.DB-IDPRCDEP
                        OR "N"         = :DB-PRCDPFLT)
                       AND IDPRCAPL    > :DB-IDREGLST.DB-IDPRCAPL
                     UNION ALL
                    SELECT IDPRCAPL
                         , DSPRCAPL
                         , IDAPLICA
                         , IDPRGGST
                         , IDDIALOG
                         , "MA"
                         , " "
                      FROM CRET904
                     WHERE IDPRCAPL LIKE :DB-PARMSINP.DB-IDPRCAPL
                       AND DSPRCAPL LIKE :DB-PARMSINP.DB-DSPRCAPL
                       AND(IDAPLICA    = :DB-PARMSINP.DB-IDAPLICA
                        OR "N"         = :DB-IDAPLFLT)
                       AND(IDDIALOG    = :DB-PARMSINP.DB-IDDIALOG
                        OR "N"         = :DB-IDDLGFLT)
                       AND("MA"        = :DB-PARMSINP.DB-IDTIPDLG
                        OR "N"         = :DB-TPDLGFLT)
                       AND(" "         = :DB-PARMSINP.DB-MCSITEXE
                        OR "N"         = :DB-STEXEFLT)
                       AND(" "         = :DB-PARMSINP.DB-IDPRCDEP
                        OR "N"         = :DB-PRCDPFLT)
                       AND IDPRCAPL    > :DB-IDREGLST.DB-IDPRCAPL
                     ORDER BY 1
                    FOR READ ONLY                              END-EXEC.

      *----------------------------------------------------------------*
       LINKAGE                     SECTION.
